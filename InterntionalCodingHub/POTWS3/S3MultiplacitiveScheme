#include <bits/stdc++.h>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    unsigned long long x;
    cin >> x;
    int iters = 0;
    while(floor(x/10) > 0){
        string k = to_string(x);
        int mult = 1;
        for (int i = 0; i < k.length(); ++i){
            mult *= (k[i] - '0');
        }
        x = mult;
        iters ++;
    }

    cout << iters;
    return 0;
}